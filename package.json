{
  "name": "skeet-example",
  "version": "0.1.0",
  "description": "Skeet App",
  "main": "dist/index.js",
  "author": "ELSOUL LABO B.V.",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "build": "npx ts-node build.ts",
    "format": "prettier --write --ignore-unknown .",
    "lint": "eslint --ext .ts,.tsx --fix .",
    "typecheck": "tsc --noEmit",
    "skeet": "run-p skeet:*",
    "skeet:openai": "yarn --cwd functions/openai dev",
    "skeet:dev": "firebase emulators:start",
    "functions:build": "yarn --cwd functions/openai build",
    "functions:dev": "firebase emulators:start --only functions",
    "functions:deploy": "firebase deploy --only functions"
  },
  "dependencies": {
    "dotenv": "16.0.3",
    "firebase-admin": "11.5.0",
    "firebase-functions": "4.4.0",
    "typesaurus": "7"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@types/node-fetch": "2.6.2",
    "babel-loader": "9.1.2",
    "esbuild": "0.17.7",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "jest": "29.3.1",
    "npm-check-updates": "16.6.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.2",
    "ts-jest": "29.0.4",
    "ts-loader": "9.4.2",
    "tsconfig-paths": "4.1.2",
    "typescript": "5.0.2"
  }
}
